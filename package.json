{
  "name": "mailgun-sending-app",
  "version": "1.0.0",
  "description": "WebApp to send parameterised E-Mails",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && parcel build --dist-dir dist --public-url . src/index.html",
    "start": "npm run clean && parcel serve src/index.html",
    "format": "prettier --write 'src/**/*'",
    "deploy": "npm run clean && npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckers-1/mailgun-sending-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beckers-1/mailgun-sending-app/issues"
  },
  "homepage": "https://github.com/beckers-1/mailgun-sending-app#readme",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "gh-pages": "^5.0.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "buffer": "^5.7.1",
    "process": "^0.11.10"
  }
}
